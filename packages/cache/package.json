{
  "name": "@encreasl/cache",
  "version": "0.1.0",
  "private": true,
  "description": "Enterprise-level caching utilities with Upstash Redis for Encreasl monorepo",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./redis": {
      "types": "./src/redis.ts",
      "default": "./src/redis.ts"
    },
    "./geospatial": {
      "types": "./src/geospatial.ts",
      "default": "./src/geospatial.ts"
    },
    "./payloadcms": {
      "types": "./src/payloadcms.ts",
      "default": "./src/payloadcms.ts"
    },
    "./types": {
      "types": "./src/types.ts",
      "default": "./src/types.ts"
    }
  },
  "scripts": {
    "build": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "test": "vitest",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@upstash/redis": "^1.34.3"
  },
  "devDependencies": {
    "@encreasl/eslint-config": "workspace:*",
    "@encreasl/typescript-config": "workspace:*",
    "@types/minimatch": "^6.0.0",
    "@types/node": "^20",
    "eslint": "^9.0.0",
    "typescript": "^5.6.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}