{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "turbo run build",
  "installCommand": "corepack enable && corepack prepare pnpm@9.12.3 --activate && pnpm install",
  "env": {
    "NODE_VERSION": "20",
    "PNPM_VERSION": "9.12.3",
    "NEXT_TELEMETRY_DISABLED": "1",
    "COREPACK_ENABLE_STRICT": "0"
  }
}
