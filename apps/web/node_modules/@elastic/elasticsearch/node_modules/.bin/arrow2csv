#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/ACER/Desktop/tap2go/node_modules/.pnpm/apache-arrow@19.0.1/node_modules/apache-arrow/bin/node_modules:/mnt/c/Users/ACER/Desktop/tap2go/node_modules/.pnpm/apache-arrow@19.0.1/node_modules/apache-arrow/node_modules:/mnt/c/Users/ACER/Desktop/tap2go/node_modules/.pnpm/apache-arrow@19.0.1/node_modules:/mnt/c/Users/ACER/Desktop/tap2go/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/ACER/Desktop/tap2go/node_modules/.pnpm/apache-arrow@19.0.1/node_modules/apache-arrow/bin/node_modules:/mnt/c/Users/ACER/Desktop/tap2go/node_modules/.pnpm/apache-arrow@19.0.1/node_modules/apache-arrow/node_modules:/mnt/c/Users/ACER/Desktop/tap2go/node_modules/.pnpm/apache-arrow@19.0.1/node_modules:/mnt/c/Users/ACER/Desktop/tap2go/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../apache-arrow@19.0.1/node_modules/apache-arrow/bin/arrow2csv.js" "$@"
else
  exec node  "$basedir/../../../../../../apache-arrow@19.0.1/node_modules/apache-arrow/bin/arrow2csv.js" "$@"
fi
