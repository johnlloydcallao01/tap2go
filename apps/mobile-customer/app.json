          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ],
    "owner": "tap2go9o",
    "extra": {
      "eas": {
        "projectId": "ef7a580f-cd18-4020-871e-aa6551eba1f0"
      }
    }
  }
}
