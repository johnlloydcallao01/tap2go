    "@tanstack/react-query": "^5.90.21",
    "abort-controller": "^3.0.0",
    "ansi-regex": "^6.1.0",
    "babel-preset-expo": "~54.0.10",
    "base64-js": "^1.5.1",
    "buffer": "^6.0.3",
