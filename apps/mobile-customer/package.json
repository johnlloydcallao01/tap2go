  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false,
        "exclude": [
          "react-native-is-edge-to-edge"
        ]
      },
      "skipDependencyValidation": true,
      "skipEnvironmentVariableValidation": true,
      "skipMetroConfigValidation": false
    },
    "autolinking": {
      "android": {
        "packageName": "com.tap2go.mobile"
      },
      "exclude": []
    }
  },
  "private": true
